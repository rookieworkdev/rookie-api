{
	"name": "rookie-api",
	"version": "1.0.0",
	"description": "Rookie API",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "tsx watch src/index.ts",
		"typecheck": "tsc --noEmit",
		"test": "NODE_ENV=test node --test tests/*.test.js",
		"test:indeed": "pnpm exec tsx src/dev/scraper-tests/runIndeedTest.ts",
		"test:linkedin": "pnpm exec tsx src/dev/scraper-tests/runLinkedInTest.ts",
		"test:af": "pnpm exec tsx src/dev/scraper-tests/runAFTest.ts",
		"test:googlemaps": "pnpm exec tsx src/dev/scraper-tests/runGoogleMapsTest.ts"
	},
	"keywords": [
		"webhook",
		"recruitment",
		"ai",
		"supabase"
	],
	"author": "Rookie AB",
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.39.7",
		"apify-client": "^2.22.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"node-cron": "^4.2.1",
		"openai": "^4.28.0",
		"resend": "^3.5.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"uuid": "^13.0.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.0",
		"@types/node-cron": "^3.0.11",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/uuid": "^11.0.0",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
